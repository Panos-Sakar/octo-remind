<Application x:Class="OctoRE.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:OctoRE"
             xmlns:converters="clr-namespace:OctoRE.Converters">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Assets/Styles/DarkStyle.xaml"/>
                <ResourceDictionary Source="/Assets/Styles/AppResources.xaml"/>
                <ResourceDictionary Source="/Assets/Styles/SidebarItem.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

</Application>
